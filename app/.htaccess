# Current active config environment
SetEnv GLOWIE_ENV development

# ----------------------------
# DO NOT EDIT BELOW THIS LINE!
# ----------------------------
# This file is responsible for handling all route requests of your application

Options -Indexes

RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php?route=$1 [NC,L,QSA]